{
  "meta": {
    "_responseSize": "#number? _.length>=1 && _.length<=2",
    "_flagMoreRecords": "#boolean",
    "_messageId": "#string",
    "_requestTimeStamp": "#string",
    "_applicationId": "string",
    "_version": "#string"
  },
  "data": [
    {
      "account": {
        "balance": {
          "available": "#number? _.length>=1 && _.length<=16",
          "availableOverdraftBalance": "#number? _.length>=1 && _.length<=16",
          "overdraftValue": "#number? _.length>=1 && _.length<=16",
          "availableOverdraftQuota": "#number? _.length>=1 && _.length<=16",
          "cash": "#number? _.length>=1 && _.length<=16",
          "unavailableClearing": "#number? _.length>=1 && _.length<=16",
          "receivable": "#number? _.length>=1 && _.length<=16",
          "blocked": "#number? _.length>=1 && _.length<=16",
          "unavailableStartDayClearingStartDay": "#number? _.length>=1 && _.length<=16",
          "cashStartDay": "#number? _.length>=1 && _.length<=16",
          "pockets": "#number? _.length>=1 && _.length<=16",
          "remittanceQuota": "#number? _.length>=1 && _.length<=16",
          "agreedRemittanceQuota": "#number? _.length>=1 && _.length<=16",
          "remittanceQuotaUsage": "#number? _.length>=1 && _.length<=16",
          "normalInterest": "#number? _.length>=1 && _.length<=16",
          "suspensionInterest": "#number? _.length>=1 && _.length<=16"
        }
      },
      "transaction": [
        {
          "id": "#string? _.length>=1 && _.length<=6",
          "postedDate": "#string",
          "description": "#string? _.length>=1 && _.length<=50",
          "amount": "#number? _.length>=1 && _.length<=16",
          "type": "#string",
          "reference1": "string? _.length>=0 && _.length<=50",
          "reference2": "string? _.length>=0 && _.length<=50",
          "reference3": "string? _.length>=0 && _.length<=50",
          "checkNumber": "string? _.length>=0 && _.length<=16"
        }
      ],
      "office": {
        "code": "string? _.length>=1 && _.length<=8",
        "name": "string? _.length>=1 && _.length<=50"
      },
      "relatedTransferAccount": {
        "type": "#string",
        "number": "#string? _.length>=1 && _.length<=16"
      },
      "customer": {
        "name": "#string? _.length>=1 && _.length<=10"
      }
    }
  ],
  "links": {
    "self": "string"
  }
}